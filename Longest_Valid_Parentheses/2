#include <iostream>
#include <string>
#include <stack>

using namespace std;


class Solution {

public:

	int longestValidParentheses(string s) {
		int len = s.length();
		stack<char>	sk;
		int pos = 0;
		while(pos < len && s[pos] == ')'){
			pos++;
		}
		for(; pos < len; pos++){
			
		}
    }

};

int main(int argc, char** argv)
{
	Solution sl;
	string s("()(()");		
    int ret = sl.longestValidParentheses(s);
	
	cout<<"Result  :("<<ret<<")"<<endl;

	return 0;

}
