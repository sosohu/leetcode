#include <iostream>
#include <string>

using namespace std;


class Solution {

public:
	inline bool isAlpha(char c){
		if(c <= 'Z' && c >= 'A')
			return true;
		if(c <= 'z' && c >= 'a')
			return true;
		return false;
	}

	bool isPalindrome(string s) {
		string tmp;
		int size = s.size();
		for(int i = 0, j = size - 1 ; i < size; i++, j--){
			while(!isAlpha(s[i])){
				i++;
			}
			while(!isAlpha(s[j])){
				j--;
			}
			if(i >= j) return true;
			if(s[i] != s[j]) return false;
		}
    }

};

int main(int argc, char** argv)
{
	Solution sl;
	string s("  hello my world ada ");		
    sl.reverseWords(s);
	
	cout<<"Result  :("<<s<<")"<<endl;

	return 0;

}
